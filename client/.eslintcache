\n                            <MDBBtn type=\"submit\" onClick={handleSection}>\r\n                              Envoyer\r\n                            </MDBBtn>\r\n                        </>\r\n                        }\r\n                        {!(titre && paragraph && file) &&\r\n                          <div>Veuillez remplir toutes les informations!</div>\r\n                        } \r\n                      </div>\r\n                  </div>\r\n                </form>\r\n              </MDBCardBody>\r\n            </MDBCard>\r\n           \r\n          </MDBCol>\r\n        </MDBRow>\r\n      </MDBContainer>\r\n    )\r\n}\r\n\r\nexport default SectionForm\r\n","C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Parametre\\ParamForm.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Parametre\\UpdateParam.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Log\\index.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Log\\SignUpProForm.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Log\\SignInProForm.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Log\\SignInAdminForm.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\data\\GoogleData.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\data\\MsgData.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Post\\DeleteCard.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Post\\CardaddPic.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Post\\LikeButton.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Post\\CardComments.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\Profil\\UpdatePicture.js",[],"C:\\Users\\m.akli\\atypikhouse_react_version\\client\\src\\components\\PageAccueil\